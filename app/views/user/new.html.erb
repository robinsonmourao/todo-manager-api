<%= form_with(model: @user, url: users_path) do |form| %>
    <div class="form-user-textbox">
        <%= form.label :username %>
        <%= form.text_field :username %>

        <%= form.label :email %>
        <%= form.text_field :email %>

        <%= form.label :password_digest %>
        <%= form.text_field :password_digest %>
    </div>
    <div>
        <%= form.submit "Cadastrar"%>
    </div>
<% end %>
