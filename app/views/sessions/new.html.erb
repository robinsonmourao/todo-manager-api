<%= form_with(model: @session, url: login_send_path) do |form| %>
    <div class="form-session-textbox">

        <%= form.label :email %>
        <%= form.text_field :email, placeholder: "Email" %>

        <%= form.label :password %>
        <%= form.password_field :password_digest, placeholder: "Senha" %>
    </div>

    <div>
        <%= form.submit "Logar" %>
    </div>
    <% end %>